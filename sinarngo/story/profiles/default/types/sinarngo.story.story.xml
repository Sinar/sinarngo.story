<?xml version="1.0"?>
<object name="sinarngo.story.story" meta_type="Dexterity FTI"
   i18n:domain="sinarngo.story"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Story</property>
 <property name="description" i18n:translate="">Story Content</property>
 <property name="icon_expr">string:${portal_url}/document_icon.png</property>
 <property name="factory">sinarngo.story.story</property>
 <property
    name="add_view_expr">string:${folder_url}/++add++sinarngo.story.story</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">True</property>
 <property name="default_view">view</property>
 <property name="view_methods">
  <element value="view"/>
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">cmf.AddPortalContent</property>
 <property name="klass">plone.dexterity.content.Item</property>
 <property name="behaviors">
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element
     value="plone.app.dexterity.behaviors.exclfromnav.IExcludeFromNavigation"/>
  <element
     value="plone.multilingualbehavior.interfaces.IDexterityTranslatable"/>
  <element
     value="plone.app.referenceablebehavior.referenceable.IReferenceable"/>
  <element
     value="collective.dexteritytextindexer.behavior.IDexterityTextIndexer"/>
  <element value="plone.app.versioningbehavior.behaviors.IVersionable"/>
  <element value="plone.app.content.interfaces.IReindexOnModify"/>
  <element value="plone.app.contenttypes.leadimage.ILeadImage"/>
  <element value="plone.app.dexterity.behaviors.metadata.ICategorization"/>
  <element value="plone.app.dexterity.behaviors.metadata.IPublication"/>
  <element value="plone.app.contenttypes.behaviors.leadimage.ILeadImage"/>
  <element value="idwf.affiliate.behavior.affiliates.IAffiliates"/>
  <element value="plone.app.dexterity.behaviors.discussion.IAllowDiscussion"/>
  <element value="sinarngo.campaign.behavior.campaigns.ICampaigns"/>
  <element value="collective.geo.behaviour.interfaces.ICoordinates"/>
  <element value="sinarngo.location.behavior.IRelatedCountries"/>
 </property>
 <property name="schema">sinarngo.story.content.story.IStory</property>
 <property
    name="model_source">&lt;model xmlns="http://namespaces.plone.org/supermodel/schema"&gt;
      &lt;schema /&gt;
  &lt;/model&gt;</property>
 <property name="model_file"></property>
 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
